- if Settings.home_page == "new"
  #home.row= render @tiles
- elsif Settings.home_page == "progress"
  - grades = Settings.qqoe_grade.split ","
  #home.row
    #progress.panel.panel-default
      .panel-body
        #aspiration= render :partial => "people/aspiration"
        #progress-bars= render :partial => "people/progress_bars"
        #dynamic-reviews= render :partial => "people/review_modal", :locals => { :grades => grades }
        #dynamic-initials= render :partial => "people/initial_modal", :locals => { :grades => grades }
        :javascript
          var topic = #{@topic.id.to_s};
        = javascript_include_tag 'cloud.min'
